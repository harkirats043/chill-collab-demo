<%- include('../partials/header') %>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Province</th>
      <th>Population</th>
    </tr>
  </thead>
  <tbody>
      <% cities.forEach((c)=>{ %>
    <tr>
        <td><%= c.city %></td>
        <td><%= c.province %></td>
        <td><%= c.population %></td>
        <td><a href="/cities/<%= c._id %>">DETAILS</a></td>
      </tr>
    <% }) %>
  </tbody>
</table>


<%- include('../partials/footer') %>